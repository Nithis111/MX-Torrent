{
  "version": 1,
  "name": "google.com",
  "home": {
    "get": "https://google.com"
  },
  "search": {
    "get": "https://www.google.com/search?q=filetype:torrent+%s",
    "js": "ee=document.querySelectorAll('a[href]'); if(ee){for(i=0;i<ee.length;i++){e=ee[i];s=e.innerHTML.replace(/<b>/ig,'').replace(/<\\/b>/ig,'');e.innerHTML=s;};};",
    "list": "h3.r [target=_blank]",
    "details": "a:regex(.*href=[\"']/url\\?q=(.*)[\"'].*)",
    "title": "a:regex(<a[^>]*>(.*)</a>)"
  }
}
