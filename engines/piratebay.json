{
  "version": 1,
  "name": "thepiratebay.org",
  "home": {
    "get": "https://thepiratebay.org/"
  },
  "search": {
    "get": "https://thepiratebay.org/search/%s/0/99/0",
    "list": "tbody tr",
    "js": "ee=document.querySelectorAll('tbody tr a[href]');for(i=0;i<ee.length;i++){e=ee[i];h=e.getAttribute('href');if(h.startsWith('/torrent')){h='https://thepiratebay.org/'+h;e.setAttribute('href',h);}};ee=document.querySelectorAll('font.detDesc');for(i=0;i<ee.length;i++){e=ee[i];h=e.innerHTML.replace(/&nbsp;/ig,' ');e.innerHTML=h;}",
    "title": "a.detLink:regex(<a[^>]*>(.*)</a>)",
    "details": "a.detLink:regex(.*href=[\"'](.*)[\"'].*)",
    "magnet": "a[href^=magnet]:regex(.*href=[\"']([^'^\"]*).*)",
    "size": "font.detDesc:regex(.*Size ([^,]*).*)",
    "seed": "regex(.*<td.*</td>.*<td.*</td>.*<td[^>]*>(.*)</td>.*<td.*</td>.*)",
    "leech": "regex(.*<td.*</td>.*<td.*</td>.*<td.*</td>.*<td[^>]*>(.*)</td>.*)"
  }
}
