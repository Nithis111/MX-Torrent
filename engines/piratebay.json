    "magnet": "a[href^=magnet]:regex(.*href=\"([^\"]*)\".*)",
    "size": "font.detDesc:regex(.*Size ([^,]*).*)",
    "seed": "td:nth-child(3):regex((.*))",
    "leech": "td:nth-child(4):regex((.*))",
    "next": "#content a[href^='/search']:last:regex(.*href=\"([^\"]*).*next.*)"
  },
