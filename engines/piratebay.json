    "get": "https://thepiratebay.org/blog",
    "list": "div.post",
    "title": "h3:regex(<h3[^>]*>(.*)</h3>)",
    "details_html": "regex((.*))"
  },
  "top": {
    "get": "tops",
    "list": "tbody tr",
    "title": "a.detLink:regex(<a[^>]*>(.*)</a>)",
